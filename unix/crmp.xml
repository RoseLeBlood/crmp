<?xml version="1.0" encoding="UTF-8"?>
<crmp>
  <Version>1.3</Version>
<!--
###################################
#Path to dsp plugins
#
-->
    <DSP>/usr/share/crmp/</DSP>
<!--
####################################
#Audio Samplerate 
#value: 44100, 48000, 96000, 192000 -->
    <Samplerate>96000</Samplerate>

<!--
###################################
#Audio Bit
#value: 8, 16, 24, 32     32=>PCMFLOAT
-->
    <Bit>32</Bit>

<!--
###################################
#Channel Konfiguration
#1: Mono; 2: Stereo;4:Quad;5:Surround;6:5.1;8:7.1; >= 0 : System
-->

    <Channel>6</Channel>
<!-- 
###################################
#Resample Mode
#0: NoResample 1: Linear; 2:Cubic; >3: Spline
-->
   <ResampleMode>1</ResampleMode>
   <Soundkarte>0</Soundkarte>
</crmp>

